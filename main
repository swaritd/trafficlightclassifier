    "def print_images_summary(image_list):\n",
    "    \"\"\"Prints basic summary of an image list\"\"\"\n",
    "    \n",
    "    labels = [image[1] for image in image_list]\n",
    "    colors = ['red', 'yellow', 'green']\n",
    "    \n",
    "    print('Images summary:')\n",
    "    print('\\tNumber of images: ', len(image_list))\n",
    "    for color in colors:\n",
    "        print('\\tNumber of {} lights: {}'.format(color, labels.count(color)))\n",
    "        \n",
    "    return\n",
    " \n",
    "\n",
    "def display_image_summary(image_list, image_no):\n",
    "    \"\"\"Displays image from list of images\"\"\"\n",
    "    \n",
    "    image = image_list[image_no][0]\n",
    "    image_label = image_list[image_no][1]\n",
    "    \n",
    "    print('Image summary:')\n",
    "    print('\\tImage shape: ', image.shape)\n",
    "    print('\\tImage label: ', image_label)\n",
    "    plt.imshow(image)\n",
    "    \n",
    "    return\n",
    "\n",
    "\n",
